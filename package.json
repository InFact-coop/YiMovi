{
  "name": "yimovi",
  "version": "1.0.0",
  "description": "Chinese Medical Humanities Film",
  "main": "web.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "test.e2e": "npm run start & nightwatch",
    "start": "node web.js",
    "startmon": "nodemon web.js",
    "startmon.watch": "nodemon web.js & npm run watch_tachyons",
    "watch_tachyons": "postcss --watch --config ./postcss.config.js",
    "build_tachyons": "postcss --config ./postcss.config.js",
    "pre-deploy": "git checkout master && git pull origin master && npm run test.e2e",
    "deploy-staging": "npm run pre-deploy && git checkout -B staging && git push origin staging",
    "deploy-production": "npm run pre-deploy && git checkout -B production && git push origin production",
    "postinstall": "npm run build_tachyons"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/InFact-coop/YiMovi.git"
  },
  "author": "Emily Bertwistle",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/InFact-coop/YiMovi/issues"
  },
  "homepage": "https://github.com/InFact-coop/YiMovi#readme",
  "dependencies": {
    "dotenv": "^4.0.0",
    "env2": "^2.2.0",
    "keystone": "^4.0.0-beta.5",
    "keystone-utils": "^0.4.0",
    "pug": "^2.0.0-rc.1",
    "lodash": "^4.17.4",
    "underscore": "^1.8.3",
    "tachyons-custom": "^4.5.4",
    "autoprefixer": "^6.7.3",
    "postcss": "^6.0.3",
    "postcss-clean": "^1.0.2",
    "postcss-cli": "^2.6.0",
    "postcss-custom-media": "^5.0.1",
    "postcss-custom-properties": "^5.0.2",
    "postcss-import": "^9.1.0"
  },
  "devDependencies": {
    "eslint": "^3.19.0",
    "nightwatch": "^0.9.16",
    "nodemon": "^1.11.0",
    "selenium-download": "^2.0.10"
  }
}
